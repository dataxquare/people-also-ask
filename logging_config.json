{
  "logger": {
    "path": "/var/logs",
    "filename": "access.log",
    "level": "debug",
    "rotation": "20 days",
    "retention": "1 months",
    "format": "[<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green>] [<level>{level}</level>] ({extra[request_id]}) - <cyan>{name}</cyan>:<cyan>{function}</cyan> - <level>{message}</level>"
  }
}